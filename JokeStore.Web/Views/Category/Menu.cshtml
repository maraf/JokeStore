@model IEnumerable<JokeStore.Web.Models.CategoryListViewModel>

@{
    Layout = null;
}

@Html.ActionLink("Home", "list", "entry")

@foreach (var link in Model)
{
    @Html.RouteLink(link.Name, new { 
        controller = "entry", 
        action = "list", 
        category = link.Url, 
        page = 1 },
        new {
            @class = link.Url == ViewBag.SelectedCategory ? "selected" : null
        }
    )
}